@model IEnumerable<Gamefacers.Models.GroupMember>

@{
    ViewBag.Title = "GroupIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GroupIndex</h2>



<div class="right-column">
    <fieldset>
        <legend> Members In Group</legend>
        @foreach (var member in Model)
        {
            <p>@member.UserId </p>
        }
        
            
        
    </fieldset>
</div>

<div class="left-column">
    <fieldset>
        <legend> Group description </legend>
        <p> @ViewBag.desc </p>
    </fieldset>
</div>

<div class="main-column">
    <fieldset>
        <p> Statusar og drasl hér </p>
    </fieldset>
</div>

<div>
    <fieldset>
        <br />
         <img src="@ViewBag.photo" height="200" width="950" /> 
    </fieldset>
</div>
